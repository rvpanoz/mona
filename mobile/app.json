{
    "builds": {
        "web": {"default": true},
        "native": {
            "packager": "cordova",
            "cordova" : {
                "config": {
                    "platforms": "android",
                    "id": "com.app.mtapp",
                    "name": "mtapp"
                }
            }
        }
    },
    "name": "mtapp",
    "indexHtmlPath": "index.html",
    "framework": "touch",
    "url": null,
    "js": [
        {
            "path": "touch/sencha-touch.js",
            "x-bootstrap": true
        },
        {
            "path": "bootstrap.js",
            "x-bootstrap": true
        },
        {
            "path": "app.js",
            "bundle": true,
            "update": "delta"
        },
        {
          "path": "config.js",
          "bundle": false,
          "update": "full"
        }
    ],
    "css": [
        {
            "path": "resources/css/app.css",
            "update": "delta"
        }
    ],
    "appCache": {
        "cache": [
            "index.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },
    "resources": [
        "resources/images",
        "resources/icons",
        "resources/startup"
    ],
    "ignore": [
        "\.svn$"
    ],
    "archivePath": "archive",
    "requires": [
    ],
    "cache": {
        "enable": true,
        "deltas": "deltas"
    },
    "id": "add54cb6-8cdf-4e08-9800-24fb0e8dfa6f"
}
